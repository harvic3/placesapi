{
  "name": "test-tybapp",
  "version": "1.0.0",
  "description": "NodeJs project using typescript",
  "main": "./dist/index.js",
  "scripts": {
    "full-test": "rm -rf coverage/* && npm run build && jest --coverage --collectCoverageFrom=src/**",
    "test": "npm run build && jest --coverage --collectCoverageFrom=src/**",
    "build": "rm -rf dist/* && tsc --sourceMap false",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "pretty-quick": "pretty-quick",
    "pretty-check": "pretty-quick --check",
    "pre-commit": "npm run lint && npm run pretty-quick --staged && npm run test",
    "start": "node dist/index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harvic3/nodetskeleton"
  },
  "author": "vickodev@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/harvic3/nodetskeleton/blob/master/README.md",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^1.0.9",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^9.1.1",
    "helmet": "^4.1.0",
    "mapper-tsk": "^1.1.1",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "resources-tsk": "^1.1.5",
    "result-tsk": "^1.0.7",
    "typeorm": "^0.2.25",
    "validator-tsk": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
